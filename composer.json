{
  "name": "nexi-checkout/php-payment-sdk",
  "description": "Nexi Checkout",
  "keywords": [
    "nexi",
    "checkout",
    "api",
    "sdk"
  ],
  "license": "MIT",
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
    "ext-json": "*",
    "psr/http-client": "*",
    "psr/http-message": "*",
    "psr/http-factory": "*"
  },
  "autoload": {
    "psr-4": {
      "NexiCheckout\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NexiCheckout\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^2.1",
    "symplify/easy-coding-standard": "^12.5",
    "rector/rector": "^2.0"
  }
}
